# AutoClip 环境变量配置

# 数据库配置
DATABASE_URL=sqlite:///./data/autoclip.db

# Redis配置
REDIS_URL=redis://redis:6379/0

# API配置 (Chaves carregadas do settings.json via UI)
# LLM_PROVIDER e API_MODEL_NAME removidos para permitir controle via interface
API_MAX_TOKENS=4096
API_TIMEOUT=60

# 处理配置
PROCESSING_CHUNK_SIZE=1500
PROCESSING_MIN_SCORE_THRESHOLD=0.3
PROCESSING_MAX_CLIPS_PER_COLLECTION=5
PROCESSING_MAX_RETRIES=3

# 日志配置
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE=backend.log

# 环境配置
ENVIRONMENT=production
DEBUG=false
